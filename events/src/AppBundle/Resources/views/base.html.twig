<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

        <title>Guppy Admin Panel</title>

        {% stylesheets
                'bootstrap/css/bootstrap.min.css'
                'https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css'
                'https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css'
                'dist/css/AdminLTE.min.css'
                'dist/css/skins/_all-skins.min.css'
                'plugins/iCheck/flat/blue.css'
                'plugins/iCheck/flat/blue.css'
                'plugins/morris/morris.css'
                'plugins/jvectormap/jquery-jvectormap-1.2.2.css'
                'plugins/datepicker/datepicker3.css'
                'plugins/daterangepicker/daterangepicker-bs3.css'
                'plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.min.css' filter='cssrewrite' %}
            <link rel="stylesheet" type="text/css" href="{{ asset_url }}" />
        {% endstylesheets %}

        <!--[if lt IE 9]>
        {% javascripts
                'https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js'
                'https://oss.maxcdn.com/respond/1.4.2/respond.min.js' %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}
        <![endif]-->

    </head>

    <body class="hold-transition skin-blue layout-top-nav">
        <div class="wrapper">

            {% include 'AppBundle::header.html.twig' %}

            <!-- Content Wrapper. Contains page content -->
            {% block content %}{% endblock %}

            {% include 'AppBundle::admin_footer.html.twig' %}
            {% include 'AppBundle::admin_sidebar_right.html.twig' %}

        </div>
        <!-- ./wrapper -->


        {% javascripts
                'plugins/jQuery/jQuery-2.2.0.min.js'
                'https://code.jquery.com/ui/1.11.4/jquery-ui.min.js' %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        <script>
            $.widget.bridge('uibutton', $.ui.button);
        </script>

        {% javascripts
                'bootstrap/js/bootstrap.min.js'
                'https://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js'
                'plugins/morris/morris.min.js'
                'plugins/sparkline/jquery.sparkline.min.js'
                'plugins/jvectormap/jquery-jvectormap-1.2.2.min.js'
                'plugins/jvectormap/jquery-jvectormap-world-mill-en.js'
                'plugins/knob/jquery.knob.js'
                'https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js'
                'plugins/daterangepicker/daterangepicker.js'
                'plugins/datepicker/bootstrap-datepicker.js'
                'plugins/bootstrap-wysihtml5/bootstrap3-wysihtml5.all.min.js'
                'plugins/slimScroll/jquery.slimscroll.min.js'
                'plugins/fastclick/fastclick.js'
                'dist/js/app.min.js'
                'dist/js/pages/dashboard.js'
                'dist/js/demo.js' %}
            <script src="{{ asset_url }}"></script>
        {% endjavascripts %}

        {% block javascriptcontent %}{% endblock %}

    </body>
</html>
